{
  "name": "comroots-backend",
  "version": "1.0.0",
  "description": "Backend for Comroots. Uses PostgreSQL, Redis, and ExpressJS. Hosted with Dokku on AWS.",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --watch . src/index.ts",
    "start": "ts-node src/index.ts",
    "env:generate": "gen-env-types .env -o env.d.ts -e .",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:initial": "run typeorm migration:generate -d src/type-orm.source.ts src/migrations/Initial",
    "migration:entities-update": "run typeorm migration:generate -d src/type-orm.source.ts src/migrations/EntitiesUpdate",
    "migration:new": "run typeorm migration:create",
    "deploy": "sh ./deploy.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^7.4.0",
    "@types/graphql-upload": "^8.0.11",
    "apollo-server-core": "^3.8.2",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.8.2",
    "argon2": "^0.28.5",
    "aws-sdk": "^2.1172.0",
    "connect-redis": "^6.1.3",
    "dataloader": "^2.1.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.0.0",
    "fireorm": "^0.23.3",
    "graphql": "^15.3.0",
    "graphql-upload": "13.0.0",
    "ioredis": "^5.1.0",
    "lodash": "^4.17.21",
    "nodemailer": "^6.7.5",
    "pg": "^8.7.3",
    "redis-session": "^0.1.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@swc/core": "^1.2.213",
    "@swc/helpers": "^0.4.3",
    "@types/connect-redis": "^0.0.18",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.41",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.4",
    "gen-env-types": "^1.3.4",
    "nodemon": "^2.0.16",
    "regenerator-runtime": "^0.13.9",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3",
    "yarn": "^1.22.19"
  },
  "packageManager": "yarn@4.0.0"
}
